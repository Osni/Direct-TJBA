<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Direct-TJBA" namespace="Direct_TJBA.Models">
  <class name="Referencia">
    <id name="Id">
      <generator class="identity"/>
    </id>
    <property name="Descricao" />
    <many-to-one name="TipoReferencia" column="TipoId" class="TipoReferencia" fetch="join" />
  </class>
</hibernate-mapping>